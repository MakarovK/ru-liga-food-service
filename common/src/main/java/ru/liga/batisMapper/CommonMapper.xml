<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ru.liga.batisMapper.CommonMapper">

    <select id="getOrderItems" resultType="ru.liga.entity.OrderItem">
        SELECT id, order_id, restaurantMenuItem_id, price, quantity
        FROM order_items
        WHERE id = #{id}
    </select>
</mapper>